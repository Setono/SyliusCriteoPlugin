{# @var \Sylius\Component\Core\Model\OrderInterface order #}
window.criteo_q.push({
    event: "trackTransaction",
    id: "{{ order.number }}",
    item: [
        {% for item in order.items %}
            { id: "{{ item.product|setono_sylius_criteo_product_id|raw }}", price: {{ item.unitPrice|setono_sylius_criteo_format_money }}, quantity: {{ item.quantity }} }{% if not loop.last %},{% endif %}
        {% endfor %}
    ]
});
